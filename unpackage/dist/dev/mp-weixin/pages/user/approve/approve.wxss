
.header {
	height: 400rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	background-color: #f0f8ff;
	border-bottom: 1rpx solid black;
}
.name, .phone {
	font-size: 100%;
}
.name {
	margin-top: 30rpx;
	margin-bottom: 20rpx;
}
.row {
	margin-top: 10rpx;
	margin-bottom: 10rpx;
	margin-left: 40rpx;
}
.pageSelecterContainer {
	width: 100%;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	background-color: white;
}
.uni-pagination {
	height: 100rpx;
	width: 500rpx;
}
.button {
	margin: 20rpx;
}

